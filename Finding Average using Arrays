#include<stdio.h>
void main()
{
int average, add = 0 ;
int i ;
int marks[10] ; /* array declaration */
for ( i = 0 ; i < 10 ; i++)
 {
	printf("\nEnter marks ") ;
	scanf("%d",&marks[i]) ; /* store data in array */
 }
 
for ( i = 0 ; i < 10 ; i++ ){
	add = add + marks[i] ; /* read data from an array*/
	average = add / 10 ;
  }
printf ( "\nAverage marks = %d \n", average ) ;
}
